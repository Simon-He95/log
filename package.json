{
  "publisher": "simonhe",
  "name": "log",
  "displayName": "log",
  "version": "0.0.9",
  "packageManager": "pnpm@8.15.9",
  "description": "Quickly generate console.log statements in your code",
  "author": "Simon He <https://github.com/Simon-He95>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Simon-He95",
  "homepage": "https://github.com/Simon-He95/log#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Simon-He95/log"
  },
  "bugs": {
    "url": "https://github.com/Simon-He95/log/issues"
  },
  "sponsor": {
    "url": "https://github.com/Simon-He95/sponsor"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.93.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.log",
        "title": "log"
      }
    ],
    "keybindings": [
      {
        "command": "extension.log",
        "key": "ctrl+l"
      }
    ]
  },
  "scripts": {
    "dev": "pnpm build --watch",
    "test": "vitest",
    "build": "tsup src/index.ts --external vscode",
    "pack": "vsce package --no-dependencies",
    "lint": "eslint --cache",
    "lint:fix": "pnpm lint --fix",
    "publish": "vsce publish --no-dependencies",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm run publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.7.1",
    "@types/node": "^18.19.50",
    "@types/vscode": "^1.93.0",
    "@vscode-use/utils": "^0.1.40",
    "@vscode/vsce": "^3.0.0",
    "bumpp": "^9.5.2",
    "eslint": "^8.57.1",
    "esno": "^0.16.3",
    "pnpm": "^8.15.9",
    "rimraf": "^4.4.1",
    "tsup": "^6.7.0",
    "typescript": "^5.6.2",
    "vitest": "^0.29.8"
  },
  "keywords": [
    "vscode",
    "extension",
    "console.log",
    "logging",
    "developer tools",
    "productivity"
  ]
}